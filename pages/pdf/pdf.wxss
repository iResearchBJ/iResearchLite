
page { background-color: #f9f9f9;padding-bottom: 56px;}
.pdf-wrap {
  padding: 30rpx;
}
.pdf-content { position: relative;}
.pdf-item { text-align: center; margin-top: 20rpx; position: relative;}
.pdf-item:first-child { margin-top: 0;}
.pdf-item image { max-width: 100%; width: 100%; background: url(data:image/gif;base64,) no-repeat 50% 50%;}

.u-return { position: fixed; right: 30rpx; bottom: 130rpx; width: 100rpx; z-index: 99999;}
.u-return .item { display: block; text-decoration: none; height: 100rpx; border-radius: 50%; background-color: rgba(0,0,0,0.5); color: #fff; text-align: center; line-height: 100rpx; font-size: 24rpx; margin-top: 10rpx; letter-spacing: 2rpx;position: relative;}
.u-return .item:first-child { margin-top: 0;}
.u-return .item:active { background-color: rgba(0,0,0,.8); color: #fff !important;}
.u-return .item .txt { display: inline-block; line-height: 1.4; margin-top: 20rpx;}
.u-return .item .nub { position: absolute; top: 0; right: 0; background-color: rgb(242, 90, 100); color: #fff; border-radius: 50%; width: 40rpx; height: 40rpx; text-align: center; line-height: 40rpx; z-index: 10;}
.u-return .item .nub.ani { -webkit-animation: myscale 300ms; animation: myscale 300ms;}
@-webkit-keyframes myscale{
  from { transform: scale(2)}
  to { transform: scale(1)}
}

@keyframes myscale{
  from { transform: scale(2)}
  to { transform: scale(1)}
}
.ico-down { display: inline-block; vertical-align: middle; width: 50rpx; height: 50rpx; background: no-repeat 50% 50% / contain; background-image: url(data:image/png;base64,)}
